(jbuild_version 1)

(executable
 ((name main)
  (modules (main gen_random  progress
            test_serialization test_validation
            test_auth test_communication test_gc))
  (libraries (lwt obus))
  (preprocess (pps (lwt.ppx)))))

(alias
 ((name   runtest)
  (action (run ${exe:main.exe}))))
